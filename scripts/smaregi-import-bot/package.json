{
  "name": "smaregi-import-bot",
  "version": "1.0.0",
  "description": "スマレジ取引履歴データをBigQueryにインポートするBot",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "sync-daily": "tsx src/index.ts --mode=daily",
    "sync-historical": "tsx src/index.ts --mode=historical"
  },
  "keywords": [
    "smaregi",
    "bigquery",
    "data-sync"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/bigquery": "^7.3.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2"
  }
}
